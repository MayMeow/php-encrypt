{
    "name": "maymeow/php-encrypt",
    "description": "Extendable Certificate Authority scirpt made with PHP",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "May Meow",
            "email": "themaymeow@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "MayMeow\\": "src",
            "MayMeow\\Cryptography\\": "app"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MayMeow\\Tests\\": "tests"
        }
    },
    "require": {
        "symfony/yaml": "^4.0|~3.0",
        "ext-openssl": "*",
        "ext-json": "*",
        "php": "7.4.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "scripts": {
        "test": "phpunit --colors=always",
        "test-coverage": "phpunit --coverage-text --colors=never"
    }
}
